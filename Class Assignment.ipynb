   ],
   "source": [
    "values=\"1,2,3,4,5,6,7,8,9\"\n",
    "vlist=values.split(\",\")\n",
    "dct={}\n",
    "for v in vlist:\n",
    "    v=v.strip()\n",
    "    #print(i,v)\n",
    "    if v in dct:\n",
    "        dct[v] += 1 #dct[v] +1\n",
    "    else:\n",
    "        dct[v] = 1\n",
    "for key,value in dct.items():\n",
    "        print(key,\":\",value)"
   ]
  }
